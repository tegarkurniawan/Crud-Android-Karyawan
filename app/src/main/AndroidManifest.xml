<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crud.rembon.crudrembon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BuatKaryawan"
            android:label="@string/title_activity_buatkaryawan">
        </activity>
        <activity android:name=".LihatKaryawan"
            android:label="@string/title_activity_lihatkaryawan">
        </activity>
        <activity android:name=".UpdateKaryawan"
            android:label="@string/title_activity_updatekaryawan">
        </activity>
    </application>


</manifest>
